<h1>Listing similar_artists</h1>

<table>
  <tr>
    <th>Artist</th>
    <th>Name</th>
    <th>Match</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @similar_artists.each do |similar_artist| %>
  <tr>
    <td><%= similar_artist.artist_id %></td>
    <td><%= similar_artist.name %></td>
    <td><%= similar_artist.match %></td>
    <td><%= link_to 'Show', similar_artist %></td>
    <td><%= link_to 'Edit', edit_similar_artist_path(similar_artist) %></td>
    <td><%= link_to 'Destroy', similar_artist, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Similar artist', new_similar_artist_path %>
